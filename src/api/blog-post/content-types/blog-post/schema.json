      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "creator": {
      "type": "string"
    },
    "thumbnail_img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "string"
    },
    "tags": {
      "type": "json",
      "required": false
    },
    "intro": {
      "type": "string"
    },
    "thumbnail_img_link": {
      "type": "string"
    }
  }
